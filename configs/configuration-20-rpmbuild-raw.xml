<?xml version="1.0" encoding="US-ASCII"?>
<configuration xmlns="http://fedorahosted.org/xmvn/CONFIG/0.6.0">
  <resolverSettings>
    <metadataRepositories>
      <repository>/usr/share/maven-fragments</repository>
    </metadataRepositories>
    <prefixes>
      <prefix>/</prefix>
    </prefixes>
  </resolverSettings>
  <repositories>
    <repository>
      <id>base-raw-pom</id>
      <type>flat</type>
      <properties>
        <root>usr/share/maven-poms</root>
      </properties>
      <stereotypes>
        <stereotype>
          <extension>pom</extension>
        </stereotype>
      </stereotypes>
    </repository>
    <repository>
      <id>base-jar</id>
      <type>jpp</type>
      <properties>
        <root>usr/share/java</root>
      </properties>
      <stereotypes>
        <stereotype>
          <extension>jar</extension>
        </stereotype>
      </stereotypes>
    </repository>
    <repository>
      <id>base-jni</id>
      <type>jpp</type>
      <properties>
        <root>usr/lib/java</root>
      </properties>
      <stereotypes>
        <stereotype>
          <extension>jar</extension>
        </stereotype>
      </stereotypes>
    </repository>
    <repository>
      <id>base-resolve</id>
      <type>compound</type>
      <configuration>
        <repositories>
          <repository>base-raw-pom</repository>
          <repository>base-jar</repository>
          <repository>base-jni</repository>
        </repositories>
      </configuration>
    </repository>
    <repository>
      <id>resolve-system</id>
      <type>compound</type>
      <properties>
        <prefix>/</prefix>
      </properties>
      <configuration>
        <repositories>
          <repository>base-resolve</repository>
        </repositories>
      </configuration>
    </repository>
    <repository>
      <id>resolve-local</id>
      <type>maven</type>
      <properties>
        <root>.m2</root>
      </properties>
    </repository>
    <repository>
      <id>resolve</id>
      <type>compound</type>
      <configuration>
        <repositories>
          <repository>resolve-local</repository>
          <repository>resolve-system</repository>
        </repositories>
      </configuration>
    </repository>
  </repositories>
</configuration>
